install.packages("maps")
install.packages("zipcode")
install.packages("ggplot2")
# Now import the libraries, load the US map data, and draw a map with all states.
library(ggplot2)
library(zipcode)
library(maps)
#load US map data
all_states <- map_data("state")
#plot all states with ggplot
p <- ggplot()
p <- p + geom_polygon( data=all_states, aes(x=long, y=lat, group = group),colour="burlywood4", fill="darkolivegreen3" )
p
# Adding US zip code data: real estate information and zip code coordinates
REdataset<-data.frame(read.csv("RealEstateZipData.csv", header = TRUE, sep = ",", quote = "\"",
dec = ".", fill = TRUE, comment.char = ""))
data(zipcode)
REdataset$RegionName = clean.zipcodes(REdataset$RegionName)
somedata = merge(REdataset, zipcode, by.x='RegionName', by.y='zip')
p <- p + geom_point( data=somedata, aes(x=longitude, y=latitude, size = X2016.04.Own, color=X2016.04.Rent)) + scale_size(name="$/sf Buy")
# p <- p + geom_text( data=somedata, hjust=0.5, vjust=-0.5, aes(x=longitude, y=latitude, label=RegionName), colour="gold2", size=4 )
p
